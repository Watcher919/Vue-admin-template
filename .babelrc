{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "env": {
    "development":{
      "plugins": ["transform-vue-jsx", "transform-runtime", "dynamic-import-node"]
    },
    "production": {
      "plugins": ["transform-vue-jsx", "transform-runtime"]
    }
  }
}
